{
  "id": null,
  "title": "Micro2 Service Overview",
  "description": "Основные метрики HTTP сервиса заказов и Kafka-сборки",
  "tags": ["order", "assembly", "http", "kafka", "microservice"],
  "timezone": "browser",
  "refresh": "5s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
  },
  "editable": true,
  "gnetId": null,
  "graphTooltip": 0,
  "links": [],
  "liveNow": false,
  "annotations": {
    "list": []
  },
  "templating": {
    "list": []
  },
  "panels": [
    {
      "id": 1,
      "title": "Order Request Rate",
      "description": "Количество HTTP-запросов в секунду к Order API",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "targets": [
        {
          "expr": "sum(rate(order_service_http_requests_total[1m])) by (method, status)",
          "legendFormat": "{{method}} {{status}}",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-uid"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "min": 0
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        },
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 2,
      "title": "Order Error Rate (5xx)",
      "description": "Количество HTTP 5xx ошибок в секунду",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "targets": [
        {
          "expr": "sum(rate(order_service_http_requests_total{status=~\"5..\"}[1m])) by (method, status)",
          "legendFormat": "{{method}} {{status}}",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-uid"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "min": 0,
          "color": {
            "mode": "palette-classic"
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "5.."
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "red"
                }
              }
            ]
          }
        ]
      }
    },
    {
      "id": 3,
      "title": "Order Request Duration p95",
      "description": "95-й перцентиль времени выполнения HTTP-запросов к Order API",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(order_service_http_request_duration_seconds_bucket[1m])) by (le, method))",
          "legendFormat": "{{method}} p95",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-uid"
          }
        },
        {
          "expr": "sum(rate(order_service_http_request_duration_seconds_sum[1m])) / sum(rate(order_service_http_request_duration_seconds_count[1m]))",
          "legendFormat": "avg",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-uid"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "min": 0
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      }
    },
    {
      "id": 5,
      "title": "Orders & Kafka Messages Rate",
      "description": "Скорость создания заказов и обработки Kafka-сообщений",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "targets": [
        {
          "expr": "rate(order_service_orders_total[1m])",
          "legendFormat": "orders created",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-uid"
          }
        },
        {
          "expr": "rate(assembly_service_kafka_messages_consumed_total[1m])",
          "legendFormat": "kafka consumed",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-uid"
          }
        },
        {
          "expr": "rate(assembly_service_kafka_messages_produced_total[1m])",
          "legendFormat": "kafka produced",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-uid"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "min": 0
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "orders created"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "blue"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "kafka consumed"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "orange"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "kafka produced"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "green"
                }
              }
            ]
          }
        ]
      }
    },
    {
      "id": 6,
      "title": "Assembly Operation Duration Average",
      "description": "Среднее время сборки заказов",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
      "targets": [
        {
          "expr": "sum(assembly_service_operation_duration_seconds_sum) / sum(assembly_service_operation_duration_seconds_count)",
          "legendFormat": "assembly avg",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-uid"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "min": 0
        }
      }
    },
    {
      "id": 7,
      "title": "Assembly Operation Duration Histogram",
      "description": "Количество заказов в каждом диапазоне времени сборки (X-axis: время сборки в секундах, Y-axis: количество заказов)",
      "type": "barchart",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
      "targets": [
        {
          "expr": "sum(assembly_service_operation_duration_seconds_bucket{le=\"5\"}) - 0",
          "legendFormat": "5",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-uid"
          },
          "instant": true
        },
        {
          "expr": "sum(assembly_service_operation_duration_seconds_bucket{le=\"6\"}) - sum(assembly_service_operation_duration_seconds_bucket{le=\"5\"})",
          "legendFormat": "6",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-uid"
          },
          "instant": true
        },
        {
          "expr": "sum(assembly_service_operation_duration_seconds_bucket{le=\"7\"}) - sum(assembly_service_operation_duration_seconds_bucket{le=\"6\"})",
          "legendFormat": "7",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-uid"
          },
          "instant": true
        },
        {
          "expr": "sum(assembly_service_operation_duration_seconds_bucket{le=\"8\"}) - sum(assembly_service_operation_duration_seconds_bucket{le=\"7\"})",
          "legendFormat": "8",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-uid"
          },
          "instant": true
        },
        {
          "expr": "sum(assembly_service_operation_duration_seconds_bucket{le=\"9\"}) - sum(assembly_service_operation_duration_seconds_bucket{le=\"8\"})",
          "legendFormat": "9",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-uid"
          },
          "instant": true
        },
        {
          "expr": "sum(assembly_service_operation_duration_seconds_bucket{le=\"10\"}) - sum(assembly_service_operation_duration_seconds_bucket{le=\"9\"})",
          "legendFormat": "10",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-uid"
          },
          "instant": true
        },
        {
          "expr": "sum(assembly_service_operation_duration_seconds_bucket{le=\"12\"}) - sum(assembly_service_operation_duration_seconds_bucket{le=\"10\"})",
          "legendFormat": "12",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-uid"
          },
          "instant": true
        },
        {
          "expr": "sum(assembly_service_operation_duration_seconds_bucket{le=\"14\"}) - sum(assembly_service_operation_duration_seconds_bucket{le=\"12\"})",
          "legendFormat": "14",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-uid"
          },
          "instant": true
        },
        {
          "expr": "sum(assembly_service_operation_duration_seconds_bucket{le=\"16\"}) - sum(assembly_service_operation_duration_seconds_bucket{le=\"14\"})",
          "legendFormat": "16",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-uid"
          },
          "instant": true
        },
        {
          "expr": "sum(assembly_service_operation_duration_seconds_bucket{le=\"18\"}) - sum(assembly_service_operation_duration_seconds_bucket{le=\"16\"})",
          "legendFormat": "18",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-uid"
          },
          "instant": true
        },
        {
          "expr": "sum(assembly_service_operation_duration_seconds_bucket{le=\"20\"}) - sum(assembly_service_operation_duration_seconds_bucket{le=\"18\"})",
          "legendFormat": "20",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-uid"
          },
          "instant": true
        }
      ],
      "transformations": [
        {
          "id": "seriesToRows",
          "options": {}
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true
            },
            "indexByName": {
              "Metric": 0,
              "Value": 1
            },
            "renameByName": {
              "Metric": "Duration",
              "Value": "Count"
            }
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "min": 0
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        },
        "xTickLabelRotation": 0,
        "xTickLabelSpacing": 0,
        "groupWidth": 0.7,
        "barRadius": 0,
        "xField": "Duration",
        "yField": "Count",
        "orientation": "vertical"
      }
    },
    {
      "id": 8,
      "title": "Kafka Message Processing Duration",
      "description": "Время обработки сообщений Kafka (p95 и среднее)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(assembly_service_kafka_message_processing_duration_seconds_bucket[1m])) by (le))",
          "legendFormat": "kafka p95",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-uid"
          }
        },
        {
          "expr": "sum(rate(assembly_service_kafka_message_processing_duration_seconds_sum[1m])) / sum(rate(assembly_service_kafka_message_processing_duration_seconds_count[1m]))",
          "legendFormat": "kafka avg",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-uid"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "min": 0
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "kafka p95"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "blue"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "kafka avg"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "orange"
                }
              }
            ]
          }
        ]
      }
    },
    {
      "id": 9,
      "title": "Orders Revenue Total",
      "description": "Накопленная суммарная выручка и общее количество созданных заказов",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
      "targets": [
        {
          "expr": "sum(order_service_orders_revenue_currency_total)",
          "legendFormat": "revenue total",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-uid"
          }
        },
        {
          "expr": "sum(order_service_orders_total)",
          "legendFormat": "orders total",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-uid"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "min": 0
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "orders total"
            },
            "properties": [
              {
                "id": "unit",
                "value": "short"
              },
              {
                "id": "custom.axisPlacement",
                "value": "right"
              },
              {
                "id": "custom.axisLabel",
                "value": "orders"
              }
            ]
          }
        ]
      }
    }
  ],
